<launch>
  <!-- Add Gazebo model path -->
  <env name="GAZEBO_MODEL_PATH" value="$(find spot_config)/model"/>
  
  <!-- Add Gazebo variables -->  
  <arg name="rviz"               default="false"/>
  <arg name="gazebo_world"       default="$(find spot_config)/worlds/outdoor_dog.world" />
  <param name="/use_sim_time"    value="true"/> 
  <arg name = "gui"              default = "true"/>


  <!-- Insert gazebo varibles -->
  <include file ="$(find spot_config)/launch/gazebo.launch">
    <arg name = "gui"               value = "$(arg gui)"/> 
  </include>
  
</launch>
